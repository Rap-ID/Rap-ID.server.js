<h1>在线登录</h1>
{{#if success}}
  <div class="row">
    <div class="col s12">
      <div class="card green white-text">
        <div class="card-content white-text">
          <span class="card-title">登录成功</span>
          <p>请等待页面跳转。</p>
        </div>
      </div>
    </div>
  </div>
  <script type="text/javascript">
    setTimeout(function() {
      console.log('{{{callback}}}');
      location.replace('{{{callback}}}');
    }, 1000);
  </script>
{{/if}}
{{#if error}}
  <div class="row">
    <div class="col s12">
      <div class="card red white-text">
        <div class="card-content white-text">
          <span class="card-title">登录失败</span>
          <p>{{error}}</p>
        </div>
      </div>
    </div>
  </div>
{{/if}}
